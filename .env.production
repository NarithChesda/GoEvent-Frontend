# Production API Configuration for Cloudflare Pages
# API hosted on Digital Ocean
VITE_API_BASE_URL=https://api.goevent.online

# Media Base URL for asset resolution
VITE_MEDIA_BASE_URL=https://api.goevent.online

# Environment identifier
VITE_ENVIRONMENT=production

# Google OAuth (replace with actual client ID in Cloudflare Pages dashboard)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Build optimization flags
VITE_BUILD_ANALYZE=false
VITE_GENERATE_SOURCEMAP=true