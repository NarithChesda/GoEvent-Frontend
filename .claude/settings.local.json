{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm doctor)",
      "Bash(npm install:*)",
      "Bash(echo $PATH)",
      "Bash(npm config:*)",
      "Bash(npm:*)",
      "WebFetch(domain:docs.anthropic.com)",
      "Bash(claude migrate-installer:*)",
      "Bash(claude update)",
      "Bash(claude config list)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(claude config set packageManager npm-global)",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(npx eslint:*)",
      "Bash(curl:*)",
      "Bash(npx vue-tsc:*)",
      "Bash(node:*)",
      "Bash(rm:*)",
      "Bash(pkill:*)",
      "Bash(true)",
      "Bash(grep:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(find:*)",
      "Bash(timeout 10 npm run dev)",
      "Bash(-H \"Content-Type: application/json\")",
      "Bash(git rm:*)",
      "Bash(git restore:*)"
    ],
    "deny": []
  }
}